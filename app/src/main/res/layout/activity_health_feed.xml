<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/colorWhite"
    tools:context="com.example.asus.mediudoc.HealthFeed">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/mediu_logo"

        android:layout_margin="20dp"
        />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="postblog"
        android:layout_margin="10dp"
        android:layout_weight="1"
        android:background="@drawable/border1"
        android:src="@drawable/addnewblog"
        android:text="Add Healthfeed" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="showblog"
        android:layout_weight="1"
        android:layout_margin="10dp"
        android:background="@drawable/border1"
        android:src="@drawable/viewhealthfeed"
        android:text="View  Healthfeed"
        android:id="@+id/imageView4" />
    </LinearLayout>
</LinearLayout>
